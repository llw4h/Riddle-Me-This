<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.riddlemethis">

    <application
        android:allowBackup="true"
        android:icon="@drawable/lowgo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/lowgo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service
            android:name="com.example.riddlemethis.MusicService"
            android:enabled="true" />

        <activity android:name="com.example.riddlemethis.Username"></activity>
        <activity android:name="com.example.riddlemethis.Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>